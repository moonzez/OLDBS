/* Body */html {     height: 100%;     }body {        margin: 0;        padding: 0;        font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;        font-size: 62.5%;        text-align: center; /* Fuer ein zentriertes Layout - Hack IE 5  */        height:100%;        background-color: #e9e9e9;        }/* Angaben nur für den Internet Explorer */* html, * html body {    margin:0;    padding:0;    height:100%;    overflow:hidden;}/* Elemente */h1 {   font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;       color: #001E4B;       font-size: 2em;       font-weight: 500;       margin: 0;       padding: 0;       }p, cite {	font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;	font-size: 0.95em;	color: #333333;	padding: 0;	margin: 5px 10px 10px 0;	line-height: 1.45em;        }li, th, td {        font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;        font-size: 0.95em;        font-weight: normal;        color: #333333;        line-height: 1.45em;        }ul {        padding: 0;        margin: 5px 10px 10px 0;        }h2 {        font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;        font-size: 1.3em;        color: #001E4B;        font-weight: 100;        margin: 0;        padding: 0;        }h3 {        font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;        color: #001E4B;        font-weight: 300;        font-size: 0.95em;        padding: 0;        margin: 0;        }h4 {        font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;        font-size: 1.6em;        color: #333333;        font-weight: 100;        margin: 0;        padding: 0;        }h5 {        margin: 0.6em 0 0.9em 0;        padding: 0;        font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;        font-size: 0.98em;        color: #333333;        }h6  {        margin: 0.6em 0 0.3em 0;        padding: 0;        font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;        font-size: 0.9em;        color: #333333;        }/* Links */a:link {        color: #D70000;        text-decoration: none;        }a:visited {        color: #D70000;        text-decoration: none;        }a:hover, a:focus {        color: #001E4B;        text-decoration: underline;        }a:active {        color: black;        }/* Klasse float:clear */.clear {        clear: both;        }#container {        /*width: 78em !important;	width: 79.6em; */        width: 798px;        margin: 3px auto 0px; /* Fuer ein zentriertes Layout - regulaere Anweisung */        padding: 0;        text-align: left; /* Zur Re-Positionierung nach dem Zentrierungs-Hack IE 5  */        position:relative;        height: 99%;        background: white;	border-left: 1px solid #bababa;	border-right: 1px solid #b3b9c4;        }/* Angaben nur für den Internet Explorer */* html #container {    padding: 0;    height:94%;    overflow:auto;}#header {        width:799px;        height: 150px;        margin-top: 0;        position: relative;        background: white;        margin-left: -1px;        }#header h1 {        margin: 10px 15px 0 15px;        padding: 0;        font-size: 1.5em;        font-weight: 500;        }#header h1 a {        color: black;        }#header h2, h3 {        margin: 0 15px 5px 15px;        padding: 0;        }#header h2 {        font-size: 1.2em;        font-weight: 500;        }#header h2 a {        color: black;        }#header h3 a {        color: black;        }#servicenav {        float: right;        padding: 0;        position: relative;	margin: 92px 17px 5px 15px;        }#servicenav a:link {        color: #e7eaef;        text-decoration: none;        }#servicenav a:visited {        color: #e7eaef;        text-decoration: none;        }#servicenav a:hover, #servicenav ul a:focus {        color: #ffffff;        text-decoration: underline;        }#servicenav a:active {        color: #e7eaef;        }#gesamt {        position:relative;	width: 798px;        margin-top: 0;        background-color: #ffffff;	border-left: 1px solid #bababa;	border-right: 1px solid #b3b9c4;        margin-left: -1px;        }.content {        width: 59.7em !important;        width: 58.5em;        max-width: 79%;        float: right;        margin: 0 0 1.6em 1.2em !important;        margin: 0 0 1.6em 0;        padding: 0;        }.ueberschrift {        margin: 0em 1em 0.2em 1em !important;        margin: 0em 1em 0.2em 0.65em;        padding: 0 0 0.1em 1.4em;        width: 34em !important;        width: 33em;        border-bottom: 1px solid #001e4b;        background-image: url(/images/rechteck_ueberschrift.gif);        background-repeat: no-repeat;        background-position: 0em 0.2em;        float:left;        color: #001e4b;        }.ueberschrifth5 {        margin: 0.4em 1em 0.2em 1em !important;        margin: 0.4em 1em 0.2em 0.65em;         padding: 0 0 0.1em 0.3em;         font-size: 1.2em;         width: 34em !important;         width: 33em;         float:left;         color: #000000;         }.inhalt2 {        margin: 0.3em 0 0 1.3em !important;        margin: 0.3em 0 0 0.8em;         padding: 0;         float: left;         font-size: 1.2em;         width: 25em !important;         width: 24em;         }.inhalt3 {        margin: 0.3em 0 0 1.3em !important;        margin: 0.3em 0 0 0.8em;         padding: 0;         float: left;         font-size: 1.2em;         width: 35.5em !important;         width: 34em;         }.inhalt4 {        margin: 0.3em 0 0 1.3em !important;        margin: 0.3em 0.4em 0 0.8em;         padding: 0;         float: left;         font-size: 1.2em;         width: 47em !important;         width: 46em;         }.inhalt-rechts {        margin: 0.8em 1.2em 2em 0.3em !important;        margin: 0.8em 1.2em 2em 0.1em;		border-left: 1px solid #b3b9c4;         padding: 0 0 0 0.9em;         float: right;         font-size: 1.2em;         width: 10.7em !important;         width: 10.7em;         }.inhalt-rechts h5 {	 margin-top: -0.1em;         }/*-------------------------        Footer        -----------------------*/#footer {        margin: 0 auto; /* Fuer ein zentriertes Layout - regulaere Anweisung */        text-align: left; /* Zur Re-Positionierung nach dem Zentrierungs-Hack IE 5  */        bottom: 0px;        background: transparent;        position: relative;        width:792px;        margin-left:-1px;        }* html #footer {    height:5%;    text-align:center;    padding-top:1%;}#fuss { position:fixed;	background-color: #596983;        margin-top: -2em !important;	margin-top: 1em;        padding: 0.2em 0.4em 0.2em 0.4em !important;        height: 2em !important;        height: 1.3em;	width: 792px;        bottom: 0px;}#fuss p {color: #222;}.breadcrumbs {        font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;        font-size: 1em;        padding: 0.1em 0 0 0;        margin-left: 19.5em;         }table#centered{}table#centered td { text-align:center;}table#centered th { background-color:#DDD }.firstrow { background-color:#DDD }ul#overview {    width: 500em;    margin: 0;     margin-left:60px;    padding: 0.8em;}ul#overview li {    list-style: none;    margin: 0.4em; padding: 0.2em;}ul#overview a { text-decoration: none; font-weight: bold;}ul#overview a:hover { color:#68838B}.fieldWithErrors {  font-family: Verdana, sans-serif;  font-size: 11pt;  color:red;  font-weight: bold;  display:inline-table;}#errorExplanation {  width: 400px;  border: 2px solid red;  padding: 7px;  padding-bottom: 12px;  margin-bottom: 20px;  background-color: #f0f0f0;}#errorExplanation h2 {  text-align: left;  font-weight: bold;  padding: 5px 5px 5px 15px;  font-size: 12px;  margin: -7px;  background-color: #c00;  color: #fff;}#errorExplanation p {  color: #333;  margin-bottom: 0;  padding: 5px;}#errorExplanation ul li {  font-size: 12px;  list-style: square;}.xujError {   margin: 1em;  text-align: left;  background-color: #f0f0f0;  font-size: 1.2em;  font-weight: bold;  color:red; } #notice {  width: 400px;  border: 2px solid red;  padding: 7px;  padding-bottom: 12px;  margin-bottom: 20px;  background: #f0f0f0;  font: bold smaller sans-serif;}label {    display: block;}#newForm {    margin: 1em 0;    padding: 0.5em;    background-color: #E7E7E7;}input {    margin: 0.5em 0;}input.edit {  background-image: url(/images/edit.png);  background-repeat: no-repeat;  background-position: center center;}input[type="submit"] {    /* margin-top: 2em; */}.newBookmark {    margin-top: 1em;    display: block;}#rubrikcontainer {         width: 14.6em !important;         width: 12em;         max-width: 19%;         float: left;         /*\*//*/ margin-left: -1em;        /**/         }.rubrik {        margin: 1.5em 0 0 1.4em !important;        margin: 1.5em 0 0 0.9em;        font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;        font-variant: normal;        font-size: 1em;        line-height: 1em;        float: left;        }.rubrik ul {        margin: 0;        width: 17.85em !important;        width: 17.8em;        float: left;        }.rubrik li {        position: relative;        list-style: none;        font-size: 1.1em;        padding: 0.3em 0.3em 0.3em 0.3em;        margin:0;        color: #333333;        width: 15.8em !important;        width: 13.5em;        line-height: 1.1em;        }        .textfeldgroesse1 { width:175px; }.textfeldgroesse2 { width:268px; }.textfeldgroesse3 { width:50px; }.textfeldgroesse4 { width:208px; }.textfeldgroesse5 { width:129px; }.textfeldgroesse6 { width:540px; }.textfeldgroesse7 { width:100px; }